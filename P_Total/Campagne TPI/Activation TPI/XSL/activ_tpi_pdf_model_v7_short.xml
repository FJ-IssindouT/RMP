    <!-- Template for images  -->
    <xsl:template name="photos_array_test">
        <xsl:param name="domain_title" />
        <!-- <xsl:param name="photos_array" /> -->
        <xsl:choose>
            <!-- <xsl:when test="$domain_title = 'Vision d&quot;ensemble signalétique Entrée d\&quot;îlot et Poteau d&quot;auvent'"> -->
            <xsl:when test="contains($domain_title, 'auvent')">
                <xsl:variable name="photos_ilot_et_auvent"><xsl:value-of select="photos_ilot_et_auvent"/></xsl:variable>
                <xsl:apply-templates select="photos_ilot_et_auvent" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue de chaque Face Appareil Distributeur Modifié (AD complet + TPI + EPEC)'">
                <xsl:variable name="photos_face_appareil_modifie"><xsl:value-of select="photos_face_appareil_modifie"/></xsl:variable>
                <xsl:apply-templates select="photos_face_appareil_modifie" />
            </xsl:when>
            <!-- <xsl:when test="$domain_title = 'Vue des bonbonnes Depass installées et des Arrêts d&quot;urgence créés'"> -->
            <xsl:when test="contains($domain_title, 'bonbonnes')">
                <xsl:variable name="photos_depass_et_arrets_urgence"><xsl:value-of select="photos_depass_et_arrets_urgence"/></xsl:variable>
                <xsl:apply-templates select="photos_depass_et_arrets_urgence" />
            </xsl:when>
            <!-- <xsl:when test="$domain_title = 'Vue du Noyau à l&quot;intérieur de la Tête Calculateur de chaque AD modifié'"> -->
            <xsl:when test="contains($domain_title, 'Calculateur')">
                <xsl:variable name="photos_noyau_interieur_tete_calculateur"><xsl:value-of select="photos_noyau_interieur_tete_calculateur"/></xsl:variable>
                <xsl:apply-templates select="photos_noyau_interieur_tete_calculateur" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue de la Palette filmée des matériels déposés'">
                <xsl:variable name="photos_palette_filmee"><xsl:value-of select="photos_palette_filmee"/></xsl:variable>
                <xsl:apply-templates select="photos_palette_filmee" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue du Switch en caisse'">
                <xsl:variable name="photos_switch_caisse"><xsl:value-of select="photos_switch_caisse"/></xsl:variable>
                <xsl:apply-templates select="photos_switch_caisse" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue des Arrivées Réseau créées au niveau de la Baie LAN WAN'">
                <xsl:variable name="photos_arrivees_reseau_baie"><xsl:value-of select="photos_arrivees_reseau_baie"/></xsl:variable>
                <xsl:apply-templates select="photos_arrivees_reseau_baie" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue des modifications réalisées au TGBT'">
                <xsl:variable name="photos_modifications_tgbt"><xsl:value-of select="photos_modifications_tgbt"/></xsl:variable>
                <xsl:apply-templates select="photos_modifications_tgbt" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Autres Photos intéressantes'">
                <xsl:variable name="photos_photos_autres"><xsl:value-of select="photos_photos_autres"/></xsl:variable>
                <xsl:apply-templates select="photos_photos_autres" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue de la fiche de Mise en service de chaque Face TPI'">
                <xsl:variable name="photos_mise_service_face_tpi"><xsl:value-of select="photos_mise_service_face_tpi"/></xsl:variable>
                <xsl:apply-templates select="photos_mise_service_face_tpi" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Fiche de Mouvement Matériel (matériels TPI/face)'">
                <xsl:variable name="photos_fiche_mouvement_materiel"><xsl:value-of select="photos_fiche_mouvement_materiel"/></xsl:variable>
                <xsl:apply-templates select="photos_fiche_mouvement_materiel" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue du Bon de livraison des TPI et des kits associés'">
                <xsl:variable name="photos_bon_livraison_tpi_kits"><xsl:value-of select="photos_bon_livraison_tpi_kits"/></xsl:variable>
                <xsl:apply-templates select="photos_bon_livraison_tpi_kits" />
            </xsl:when>
            <xsl:when test="$domain_title = 'Vue de la fiche de Mise en service TPI installé, signée par le gérant'">
                <xsl:variable name="photos_fiche_mise_service_tpi"><xsl:value-of select="photos_fiche_mise_service_tpi"/></xsl:variable>
                <xsl:apply-templates select="photos_fiche_mise_service_tpi" />
            </xsl:when>
            <xsl:otherwise></xsl:otherwise>
        </xsl:choose>
    </xsl:template>

